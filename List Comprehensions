celcius = [0, 10, 20, 30, 40]
fahrenheit = [( (9/5)*temp + 32) for temp in celcius]

mylist = []
for x in [2,4,6]:
  for y in [1,10,1000]:
    mylist.append(x*y)
mylist = [x*y for x in [2,4,6] for y in [1,10,1000]
