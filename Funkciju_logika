def nelyginiai(sarasas):
    nelyginiu_sarasas = []
    lyginiu_sarasas = []
    for skaicius in sarasas:
        if skaicius % 2 != 0:
            nelyginiu_sarasas.append ( skaicius )
        else:
            lyginiu_sarasas.append ( skaicius )
    return (nelyginiu_sarasas, lyginiu_sarasas)


print (nelyginiai([1, 2, 3, 4, 5, 6, 7, 8, 9]))
